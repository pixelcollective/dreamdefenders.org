{
  "name": "@dream-defenders/theme",
  "version": "1.0.0",
  "license": "MIT",
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "mix",
    "build:production": "mix --production",
    "start": "mix watch",
    "hot": "mix watch --hot",
    "clear": "wp acorn optimize:clear",
    "test": "npm run lint",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint resources/scripts",
    "lint:css": "stylelint \"resources/**/*.{css,scss,vue}\"",
    "translate": "npm run translate:pot && npm run translate:js",
    "translate:pot": "wp i18n make-pot . ./resources/lang/sage.pot --ignore-domain --include=\"app,resources\"",
    "translate:js": "wp i18n make-json ./resources/lang --pretty-print"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.4.1",
    "@tinypixelco/laravel-mix-wp-blocks": "^1.1.0",
    "@wordpress/babel-preset-default": "^6.1.0",
    "@wordpress/base-styles": "^1.9.0",
    "@wordpress/block-library": "^3.2.6",
    "@wordpress/browserslist-config": "^3.0.1",
    "@wordpress/components": "^9.2.1",
    "@wordpress/dependency-extraction-webpack-plugin": "^3.0.1",
    "@wordpress/dom-ready": "^2.5.1",
    "@wordpress/edit-post": "^3.13.1",
    "@wordpress/postcss-themes": "^2.4.0",
    "babel-eslint": "^10.1.0",
    "browser-sync": "^2.26.14",
    "browser-sync-webpack-plugin": "^2.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.27.0",
    "eslint-plugin-import": "^2.22.1",
    "laravel-mix": "^6.0.11",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1",
    "postcss": "^8.2.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-with-direction": "^1.3.1",
    "rimraf": "^3.0",
    "sass": "^1.34.0",
    "sass-loader": "11.0.1",
    "stylelint": "^13.10.0",
    "stylelint-config-standard": "^20.0.0",
    "tailwindcss": "^2.0.3",
    "tailwindcss-blend-mode": "^1.0.0",
    "tailwindcss-gradients": "^3.0.0",
    "tailwindcss-multi-column": "^1.0.2",
    "tailwindcss-scrims": "^1.0.0",
    "tailwindcss-transitions": "^2.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "@tinypixelco/hoverfx": "^0.0.2",
    "alpinejs": "^2.8.2",
    "animejs": "^3.2.1",
    "headroom.js": "^0.11.0",
    "intersection-observer": "^0.10.0",
    "lozad": "^1.14.0"
  }
}
