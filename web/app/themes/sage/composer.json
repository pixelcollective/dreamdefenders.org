{
  "name": "dreamdefenders/theme",
  "type": "wordpress-theme",
  "license": "MIT",
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:kellymears/sage-svg"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "require": {
    "php": "^7.2",
    "log1x/sage-directives": "^1.1",
    "kellymears/sage-svg": "dev-master",
    "tiny-pixel/acorn-instagram": "dev-master",
    "roots/acorn": "dev-master"
  },
  "require-dev": {
    "filp/whoops": "^2.5",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "lint": [
      "phpcs --ignore=vendor,resources,storage,dist --extensions=php --standard=PSR12 ."
    ],
    "post-autoload-dump": [
      "Roots\\Acorn\\ComposerScripts::postAutoloadDump"
    ]
  }
}
