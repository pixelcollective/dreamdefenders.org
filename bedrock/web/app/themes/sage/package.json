{
  "name": "sage",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production run-s mix translate",
    "start": "cross-env NODE_ENV=development run-s \"mix --watch\"",
    "hot": "cross-env NODE_ENV=development run-s build mix:hot",
    "mix": "webpack --progress --hide-modules --config=./../../../../node_modules/laravel-mix/setup/webpack.config.js",
    "mix:hot": "webpack-dev-server --inline --hot --config=./../../../../node_modules/laravel-mix/setup/webpack.config.js",
    "test": "run-s -c lint",
    "translate": "run-s translate:pot translate:js",
    "translate:pot": "wp i18n make-pot ./ ./resources/languages/support.pot --ignore-domain",
    "translate:js": "wp i18n make-json ./resources/languages --no-purge --pretty-print",
    "wp-scripts:build": "wp-scripts build",
    "wp-scripts:check-engines": "wp-scripts check-engines",
    "wp-scripts:start": "wp-scripts start",
    "wp-scripts:test:e2e": "wp-scripts test-e2e",
    "wp-scripts:test:unit": "wp-scripts test-unit-js"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@wordpress/scripts": "^3.3.0",
    "autoprefixer": "^9.6.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "2.2.2",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "glhd-tailwindcss-transitions": "^0.3.0",
    "laravel-mix": "^4.0.15",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.6.0",
    "resolve-url-loader": "2.3.1",
    "run-s": "^0.0.0",
    "tailwindcss": "^1.0.5",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4"
  },
  "dependencies": {
    "@wordpress/dom-ready": "^2.5.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
