{
  "name": "roots/clover",
  "type": "wordpress-plugin",
  "license": "MIT",
  "description": "WordPress plugin boilerplate with a modern development workflow",
  "homepage": "https://roots.io/",
  "authors": [
    {
      "name": "Nathan Knowler",
      "email": "nathan@knowler.me",
      "homepage": "https://github.com/knowler"
    }
  ],
  "keywords": [
    "wordpress",
    "plugin",
    "boilerplate"
  ],
  "support": {
    "issues": "https://github.com/roots/clover/issues",
    "forum": "https://discourse.roots.io/"
  },
  "autoload": {
    "psr-4": {
      "TinyPixel\\App\\": "app",
      "TinyPixel\\": "src",
      "Roots\\Clover\\": "src/Clover",
      "PostTypes\\Schemas\\": "app/PostTypes"
    },
    "files": [
      "app/post-types.php",
      "app/acf-form.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "TinyPixel\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.1",
    "composer/installers": "^1.0",
    "roots/acorn": "dev-master",
    "roots/support": "dev-master",
    "illuminate/support": "^5.7",
    "jjgrainger/posttypes": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "scripts": {
    "lint": "phpcs",
    "test": "echo 'The road to hell is paved with good intentions.'"
  }
}
